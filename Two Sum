/*leetcode two sum   


arr[]={3,4,5,6,7,8,9}  target=9  

any combination in arr to give sum 9*/
#include<stdio.h>

int main()
{
    int a[]={3,4,5,6,7,8,9};
    int i,j;
    for(i=0;i<7;i++)
    {
        for(j=i+1;j<7;j++)
        {
            if(a[i]+a[j]==9)
            {
                printf("%d   %d\n",a[i],a[j]);
            }
        }
    }
    return 0;
}
